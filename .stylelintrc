{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind",
        "apply",
      ]
    }],
    "block-no-empty": null,
    "declaration-empty-line-before": null,
    "unit-whitelist": ["em", "rem", "s", "px", "%"]
  }
}